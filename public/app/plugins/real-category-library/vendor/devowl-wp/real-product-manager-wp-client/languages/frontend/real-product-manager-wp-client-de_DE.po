msgid "Activated %s"
msgstr "%s aktiviert"

#: index.js:1
msgid "Telemetry data sharing enabled"
msgstr "Teilen von Telemetrie-Daten aktiviert"

#: index.js:1
msgid "Telemetry data sharing disabled"
msgstr "Teilen von Telemetrie-Daten deaktiviert"

#: index.js:1
msgid "Your license key"
msgstr "Dein Lizenzschlüssel"

#: index.js:1
msgid "Your installation is fully activated."
msgstr "Deine Installation ist vollständig aktiviert."

#: index.js:1
msgid "Show announcements for this plugin"
msgstr "Ankündigungen für dieses Plugin anzeigen"

#: index.js:1
msgid "Save"
msgstr "Speichern"

#: index.js:1
msgid "License settings"
msgstr "Lizenzeinstellungen"

